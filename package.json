  },
  "keywords": [
    "url",
    "uri"
  ],
  "author": "Julien Dargelos",
  "license": "MIT",
  "dependencies": {
    "parameters-js": "^1.0.14",
    "pathname-js": "^1.0.5"
  },
  "devDependencies": {
    "browserify": "^14.5.0",
    "jsdoc-to-markdown": "^3.0.2"
  }
}

{
  "name": "open-url-js",
  "version": "1.0.13",
  "description": "Simply manage urls.",
  "main": "index.js",
  "scripts": {
    "test": "node ./test.js",
    "build": "browserify index.js > dist/open-url.js",
    "doc": "jsdoc2md index.js --template README.hbs --partial partials/*.hbs > README.md",
